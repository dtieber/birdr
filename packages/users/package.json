{
  "name": "@birdr/users",
  "private": true,
  "scripts": {
    "build": "tsc",
    "debug": "node --inspect dist/index.js",
    "knex": "knex",
    "lint": "eslint . --fix --ext .js,.ts,.tsx --cache --max-warnings 0",
    "serve": "node dist/index.js",
    "test": "pnpm test:ut && pnpm test:it",
    "test:it": "jest --testRegex '.*\\.itest\\.ts$' --run-in-band",
    "test:ut": "jest --testRegex '.*\\.test\\.ts$' --passWithNoTests",
    "watch": "nodemon --inspect src/index.ts"
  },
  "dependencies": {
    "@birdr/shared": "workspace:*",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "fastify": "^4.24.3",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "pino": "^8.16.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fastify-plugin": "^4.5.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
